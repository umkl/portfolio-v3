  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "portfolio-v3 dev",
      "url": "http://localhost:3000",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "/usr/local/bin/npm",
      "args": ["run","dev"],
      "skipFiles": ["<node_internals>/**"],
    },
    {
      "name": "portfolio-v3 prod",
      "request": "launch",
      "runtimeArgs": ["run build"],
      "cwd": "/Users/michaelungar/Projects/portfolio-v3",
      "runtimeExecutable": "/usr/local/bin/node",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name":"chrome debugger",
      "type":"chrome",
      "request":"launch",
      "url":"http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
